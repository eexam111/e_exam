<div class="section">
    <div class="message">
        <h1>Available MCQ Tests</h1>
    </div>
</div>

<div class="container">
    <ul class="mcq-tests-list">
        <% mcqTests.forEach((mcqTest) => { %>
            <li>
                <a href="/mcqattend/<%= mcqTest._id %>" class="test-link"><%= mcqTest.name %></a>
            </li>
        <% }) %>
    </ul>
</div>
